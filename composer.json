{
  "name": "outl1ne/nova-multiselect-filter",
  "description": "Multiselect filter for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "filter",
    "laravel-nova",
    "multiselect",
    "select"
  ],
  "authors": [
    {
      "name": "Kaspar Rosin",
      "email": "kaspar.rosin@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Outl1ne",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "outl1ne/nova-translations-loader": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "Outl1ne\\NovaMultiselectFilter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outl1ne\\NovaMultiselectFilter\\FilterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
