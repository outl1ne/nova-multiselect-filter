{
  "name": "optimistdigital/nova-multiselect-filter",
  "description": "Multiselect filter for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "filter",
    "laravel-nova",
    "multiselect",
    "select"
  ],
  "authors": [
    {
      "name": "Kaspar Rosin",
      "email": "kaspar.rosin@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "laravel/nova": "^3.0",
    "optimistdigital/nova-translations-loader": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigtal\\NovaMultiselectFilter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigtal\\NovaMultiselectFilter\\FilterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
